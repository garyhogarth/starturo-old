{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["es2017"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": "./",
    "paths": {
      "#starturo/*": ["src/*"],
      "#assets/*": ["src/app/assets/*"]
    }
  },
  "exclude": ["node_modules", "babel.config.js", "metro.config.js", "_jest.config.js"]
}
